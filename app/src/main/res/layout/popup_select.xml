                android:text="안도하는"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
        </TableRow>
        <TableRow
            android:gravity="center">
            <Button
                android:id="@+id/irritatedF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="성가신"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/dismayedf"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="낭패한"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/nervousf"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="신경 쓰임"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/aggrievedF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="억울한"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
