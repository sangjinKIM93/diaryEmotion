                android:id="@+id/happyF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:textStyle="bold"
                android:text="기쁨"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
        </TableRow>
        <TableRow
            android:layout_marginBottom="5dp"
            android:gravity="center">
            <Button
                android:id="@+id/grumpyF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="툴툴대는"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/disappointF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="실망한"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/afraidF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="두려운"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
            <Button
                android:id="@+id/jealousF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="질투하는"
