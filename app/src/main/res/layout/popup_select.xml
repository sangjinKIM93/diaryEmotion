            <Button
                android:id="@+id/confidentF"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:background="@color/whiteSmoke"
                android:text="자신하는"
                android:textSize="15dp"
                android:gravity="center"
                android:layout_margin="1dp"/>
